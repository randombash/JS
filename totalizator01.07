var iloscLiczb = 5;                     // ilośc liczb do wylosowania
var liczbaMin = 2;                      // minimalna losowana liczba
var liczbaMax = 99;                     // maksymalna losowana liczba
var powtarzanie = true;                 // czy liczby mogą się powtarzać
var wynik = [];                         // zbiór wylosowanych liczb w losowej kolejności
var liczba = '';                        // wylosowana liczba
var znalezione = '';
var nieznalezione = '';

function wylicz() {                     // generuje losową liczbę w podanym zakresie
	    var liczba = Math.round(Math.random() * liczbaMax) + liczbaMin; 
	    return liczba;
	    }

	do {
     wylicz();                          // sprawdzić continue, spr czy do się aktualizuje na różnych etapach
    
for (i = 0; i <= wynik.length; i++) {   // pętla sprawdzajca powtarzanie liczb w wyniku
    if (wynik[i] == liczba) {
        znalezione = true;
    } else {
        nieznalezione = true;
    }
}                                       // powrót do pętli do
    if (powatrzanie = false && iloscLiczb > (liczbaMax - liczbaMin)) {
	  wynik.push('Nie można wylosować liczb w podanym zakresie');
   } else if (powtarzanie = false && znalezione != true) {
       wynik.push(liczba);
   } else if (powtarzanie = true) {
       wynik.push(liczba);
   } else {
       i = i--;
   } 
   } while (wynik.length < iloscLiczb);
   
   console.log(wynik);